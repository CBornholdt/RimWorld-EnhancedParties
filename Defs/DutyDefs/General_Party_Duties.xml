<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <DutyDef>
  	<defName>EP_MakeThingsToFocus</defName>
  	<socialModeMax>Off</socialModeMax>
  	<hook>MediumPriority</hook>
  	<thinkNode Class="ThinkNode_Priority">
  		<subNodes>

  			<li Class="DutyConditional_HasEnoughThingsAvailable">
  				<subNodes>

  					<li Class="DutyConditional_HasEnoughThingsAtFocus">
  						<subNodes>
  							<li Class="ThinkNode_DutyTaskComplete">
  							</li>
  						</subNodes>
  					</li>

  					<li Class="DutyConditional_HasEnoughThingsAtFocus">
  						<invert>true</invert>
  						<subNodes>
  							<li Class="DutyJob_BringThingsToFocus">
  							</li>
  						</subNodes>
  					</li>

  				</subNodes>
  			</li>

  			<li Class="DutyConditional_HasEnoughThingsAvailable">
  				<invert>true</invert>
  				<subNodes>
  					<li Class="DutyJob_PerformDutyRecipe">
  					</li>
  				</subNodes>
  			</li>

  			<li Class="ThinkNode_Logger">
  				<subNodes>
  					<li Class="DutyJob_WanderNearFocus">
  					</li>
  				</subNodes>
  			</li>

  		</subNodes>
  	</thinkNode>
  </DutyDef>
</Defs>
